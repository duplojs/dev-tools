{
  "name": "@duplojs/dev-tools",
  "version": "0.0.0",
  "author": "duplojs",
  "contributors": [
    {
      "name": "zeriix",
      "url": "https://github.com/zeriix"
    },
    {
      "name": "mathcovax",
      "url": "https://github.com/mathcovax"
    }
  ],
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duplojs/dev-tools.git"
  },
  "scripts": {
    "build": "rollup --config",
    "test:tu": "vitest",
    "test:tu:watch": "vitest --watch",
    "test:tu:update": "vitest --update",
    "test:types": "tsc -p tsconfig.test.json",
    "test:lint": "eslint",
    "test:lint:fix": "eslint --fix",
    "prepare": "husky"
  },
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "exports": {
    "./generateMetadata": {
      "import": "./dist/generateMetadata/index.mjs",
      "require": "./dist/generateMetadata/index.cjs",
      "types": "./dist/generateMetadata/index.d.ts"
    },
    "./declarationIncludes": {
      "import": "./dist/declarationIncludes/index.mjs",
      "require": "./dist/declarationIncludes/index.cjs",
      "types": "./dist/declarationIncludes/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@duplojs/eslint": "0.5.0",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/node": "24.3.0",
    "eslint": "9.34.0",
    "husky": "9.1.7",
    "rollup": "4.50.0",
    "rollup-plugin-delete": "3.0.1",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-tsc-alias": "1.1.4",
    "tslib": "2.8.1",
    "tsx": "4.20.5",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@duplojs/utils": "1.3.21"
  },
  "workspaces": [
    "integration"
  ],
  "keywords": [],
  "engines": {
    "node": ">=22.15.1"
  }
}
